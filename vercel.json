{
  "version": 2,
  "builds": [
    {
      "src": "api/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "index.html",
      "dest": "index.html",
      "headers": {
        "Cache-Control": "s-maxage=0"
      }
    }
  ]
}
