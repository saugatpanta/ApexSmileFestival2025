{
  "version": 2,
  "builds": [
    {
      "src": "api/health.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/sync.js",
      "use": "@vercel/node"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "api/health",
      "dest": "api/health"
    },
    {
      "src": "api/sync",
      "dest": "api/sync"
    },
    {
      "src": "/(.*)",
      "dest": "index.html",
      "headers": {
        "Content-Type": "text/html"
      }
    }
  ]
}
