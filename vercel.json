{
  "version": 2,
  "builds": [
    {
      "src": "api/health.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/registrations.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/sync.js",
      "use": "@vercel/node"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/health",
      "dest": "/api/health.js",
      "methods": ["GET"]
    },
    {
      "src": "/api/registrations",
      "dest": "/api/registrations.js",
      "methods": ["POST"]
    },
    {
      "src": "/api/sync",
      "dest": "/api/sync.js",
      "methods": ["GET"]
    },
    {
      "src": "/(.*)",
      "dest": "index.html",
      "headers": {
        "Content-Type": "text/html",
        "Cache-Control": "public, max-age=3600"
      }
    }
  ]
}
